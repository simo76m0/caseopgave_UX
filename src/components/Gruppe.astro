---
import ProgramCards from "./ProgramCards.astro";
const { program } = Astro.props;
---

{
  program.map((program) => (
    <ProgramCards
      link={"/program/" + program.slug}
      navn={program.navn}
      description={program.description}
      tid={program.tid}
      img={`/imgs/${program.img}`}
    />
  ))
}
